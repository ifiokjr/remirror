{
  "private": true,
  "name": "@remirror/e2e",
  "description": "E2E tests with cypress",
  "version": "0.0.0",
  "scripts": {
    "prebuild": "run-s clean lint",
    "build:DEPRECATED": "webpack --output-filename out.js --entry ./cypress/integration/start.e2e.ts",
    "clean": "rimraf out.js",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "dev:DEPRECATED": "webpack --output-filename out.js --entry ./cypress/integration/start.e2e.ts --watch",
    "lint": "tslint --project tsconfig.json --config ../../tslint.json",
    "typecheck": "tsc --pretty --noEmit"
  },
  "dependencies": {
    "@cypress/webpack-preprocessor": "4.0.3",
    "cypress": "3.1.5",
    "cypress-pipe": "1.3.3",
    "cypress-plugin-snapshots": "1.2.5"
  }
}
