{
  "name": "docusaurus-plugin-examples",
  "version": "0.0.0",
  "private": true,
  "description": "A docusaurus plugin",
  "keywords": [
    "remirror"
  ],
  "homepage": "https://github.com/remirror/remirror/tree/HEAD/docusaurus-plugin-examples/docusaurus-plugin-examples",
  "repository": "https://github.com/remirror/remirror/tree/HEAD/packages/docusaurus-plugin-examples",
  "license": "MIT",
  "contributors": [
    "Ifiok Jr. <ifiokotung@gmail.com>"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/docusaurus-plugin-examples.esm.js",
      "require": "./dist/docusaurus-plugin-examples.cjs.js",
      "browser": "./dist/docusaurus-plugin-examples.esm.js",
      "types": "./dist/docusaurus-plugin-examples.cjs.d.ts",
      "default": "./dist/docusaurus-plugin-examples.esm.js"
    },
    "./components": {
      "import": "./components/dist/docusaurus-plugin-examples-components.esm.js",
      "require": "./components/dist/docusaurus-plugin-examples-components.cjs.js",
      "browser": "./components/dist/docusaurus-plugin-examples.esm.js",
      "types": "./components/dist/docusaurus-plugin-examples-components.cjs.d.ts",
      "default": "./components/dist/docusaurus-plugin-examples-components.esm.js"
    },
    "./package.json": "./package.json",
    "./types/*": "./dist/declarations/src/*.d.ts"
  },
  "main": "dist/docusaurus-plugin-examples.cjs.js",
  "module": "dist/docusaurus-plugin-examples.esm.js",
  "types": "dist/docusaurus-plugin-examples.cjs.d.ts",
  "files": [
    "src",
    "dist",
    "components"
  ],
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "@docusaurus/core": "^2.0.0-beta.3",
    "@docusaurus/module-type-aliases": "^2.0.0-beta.3",
    "@docusaurus/theme-classic": "^2.0.0-beta.3",
    "@docusaurus/types": "^2.0.0-beta.3",
    "@docusaurus/utils": "^2.0.0-beta.3",
    "@remirror/core-helpers": "^1.0.1",
    "create-context-state": "^1.0.0",
    "globby": "^11.0.4",
    "react-intersection-observer": "^8.32.0",
    "react-syntax-highlighter": "^15.4.3",
    "reakit": "^1.3.8",
    "sucrase": "^3.20.0"
  },
  "devDependencies": {
    "@remirror/pm": "^1.0.1",
    "@remirror/react": "^1.0.1",
    "@types/react": "^17.0.14",
    "@types/react-syntax-highlighter": "^13.5.2",
    "react": "^17.0.2"
  },
  "peerDependencies": {
    "@remirror/pm": "^1.0.1",
    "@types/react": "^16.14.0 || ^17 || ^18",
    "react": "^16.14.0 || ^17 || ^18"
  },
  "publishConfig": {
    "access": "public"
  },
  "@remirror": {
    "tsconfigs": {
      "src": {
        "compilerOptions": {
          "types": [
            "node"
          ]
        }
      }
    }
  },
  "preconstruct": {
    "entrypoints": [
      "index.ts",
      "components/index.ts"
    ]
  }
}
