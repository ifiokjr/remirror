{
  "name": "@remirror/cli",
  "version": "1.0.1",
  "description": "The remirror cli",
  "homepage": "https://github.com/remirror/remirror/tree/HEAD/packages/remirror__cli",
  "repository": "https://github.com/remirror/remirror/tree/HEAD/packages/remirror__cli",
  "license": "MIT",
  "contributors": [
    "Ifiok Jr. <ifiokotung@gmail.com>"
  ],
  "sideEffects": true,
  "exports": {
    ".": {
      "require": "./dist/remirror-cli.cjs.js",
      "types": "./dist/remirror-cli.cjs.d.ts"
    },
    "./cli": {
      "require": "./cli/dist/remirror-cli-cli.cjs.js"
    },
    "./package.json": "./package.json",
    "./types/*": "./dist/declarations/src/*.d.ts"
  },
  "main": "dist/remirror-cli.cjs.js",
  "types": "dist/remirror-cli.cjs.d.ts",
  "bin": {
    "remirror": "bin.js"
  },
  "files": [
    "cli",
    "bin",
    "dist"
  ],
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "@remirror/core-helpers": "^1.0.1",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@types/fs-extra": "^9.0.11",
    "@types/ms": "^0.7.31",
    "@types/update-notifier": "^5.0.0",
    "@types/yup": "^0.29.11",
    "chalk": "^4.1.0",
    "cli-spinners": "^2.6.0",
    "clipanion": "^3.0.0-rc.11",
    "cpy": "^8.1.1",
    "dargs": "^8.0.0",
    "execa": "^5.1.1",
    "figures": "^3.2.0",
    "fs-extra": "^10.0.0",
    "globby": "^11.0.4",
    "ink": "^3.0.8",
    "ink-link": "^2.0.0",
    "ink-multi-select": "^2.0.0",
    "ink-spinner": "^4.0.0",
    "jsesc": "^3.0.1",
    "json.macro": "^1.3.0",
    "ms": "^2.1.3",
    "react": "^17.0.2",
    "react-use": "^17.2.3",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.2",
    "rollup-plugin-terser": "^7.0.2",
    "terminal-link": "^2.1.1",
    "typanion": "^3.3.0",
    "update-notifier": "^5.1.0"
  },
  "devDependencies": {
    "@remirror/core-types": "^1.0.1",
    "@testing-library/react-hooks": "^7.0.0",
    "@types/ink-spinner": "^3.0.0",
    "@types/ink-testing-library": "^1.0.1",
    "@types/jsesc": "^3.0.0",
    "@types/node": "^15.12.4",
    "@types/react": "^17.0.11",
    "@types/rimraf": "^3.0.0",
    "ink-testing-library": "^2.1.0"
  },
  "engines": {
    "node": ">=8"
  },
  "preferGlobal": true,
  "publishConfig": {
    "access": "public"
  },
  "@remirror": {
    "tsconfigs": {
      "src": {
        "compilerOptions": {
          "allowJs": true
        }
      }
    }
  },
  "preconstruct": {
    "entrypoints": [
      "index.ts",
      "cli.ts"
    ]
  },
  "rn:dev": "src/index.ts"
}
