{
  "name": "@remirror/extension-callout",
  "version": "1.0.0-next.60",
  "description": "Add callouts to your text editor.",
  "keywords": [
    "remirror",
    "extension"
  ],
  "homepage": "https://github.com/remirror/remirror/tree/HEAD/packages/remirror__extension-callout",
  "repository": "https://github.com/remirror/remirror/tree/HEAD/packages/remirror__extension-callout",
  "license": "MIT",
  "contributors": [
    "Will Hawker <w.hawker@hotmail.co.uk>"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/remirror-extension-callout.esm.js",
      "require": "./dist/remirror-extension-callout.cjs.js",
      "browser": "./dist/remirror-extension-callout.browser.esm.js",
      "types": "./dist/remirror-extension-callout.cjs.d.ts",
      "default": "./dist/remirror-extension-callout.esm.js"
    },
    "./package.json": "./package.json",
    "./types/*": "./dist/declarations/src/*.d.ts"
  },
  "main": "dist/remirror-extension-callout.cjs.js",
  "module": "dist/remirror-extension-callout.esm.js",
  "browser": {
    "./dist/remirror-extension-callout.cjs.js": "./dist/remirror-extension-callout.browser.cjs.js",
    "./dist/remirror-extension-callout.esm.js": "./dist/remirror-extension-callout.browser.esm.js"
  },
  "types": "dist/remirror-extension-callout.cjs.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@babel/runtime": "^7",
    "@linaria/core": "^3.0.0-beta.1",
    "@remirror/core": "1.0.0-next.60",
    "@remirror/messages": "0.0.0",
    "type-fest": "^0.21.2"
  },
  "devDependencies": {
    "@remirror/pm": "1.0.0-next.60"
  },
  "peerDependencies": {
    "@remirror/pm": "1.0.0-next.60"
  },
  "publishConfig": {
    "access": "public"
  },
  "@remirror": {
    "sizeLimit": "5 KB"
  },
  "rn:dev": "src/index.ts"
}
