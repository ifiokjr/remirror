{
  "name": "@remirror/docgen",
  "version": "0.0.0",
  "private": true,
  "description": "Generate documentation for the remirror project.",
  "keywords": [
    "remirror"
  ],
  "homepage": "https://github.com/remirror/remirror/tree/HEAD/remirror__docgen/docgen",
  "repository": "https://github.com/remirror/remirror/tree/HEAD/packages/remirror__docgen",
  "license": "MIT",
  "contributors": [
    "Ifiok Jr. <ifiokotung@gmail.com>"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/remirror-docgen.esm.js",
      "require": "./dist/remirror-docgen.cjs.js",
      "browser": "./dist/remirror-docgen.browser.esm.js",
      "types": "./dist/remirror-docgen.cjs.d.ts",
      "default": "./dist/remirror-docgen.esm.js"
    },
    "./package.json": "./package.json",
    "./types/*": "./dist/declarations/src/*.d.ts"
  },
  "main": "dist/remirror-docgen.cjs.js",
  "module": "dist/remirror-docgen.esm.js",
  "browser": {
    "./dist/remirror-docgen.cjs.js": "./dist/remirror-docgen.browser.cjs.js",
    "./dist/remirror-docgen.esm.js": "./dist/remirror-docgen.browser.esm.js"
  },
  "types": "dist/remirror-docgen.cjs.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "debug": "nodemon --inspect ./"
  },
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "@microsoft/tsdoc": "^0.13.2",
    "@microsoft/tsdoc-config": "^0.15.2",
    "@remirror/core-helpers": "1.0.0-next.60",
    "@types/node": "^15.12.5",
    "read-pkg-up": "^7.0.1",
    "ts-morph": "^11.0.0",
    "tslog": "^3.1.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.7"
  },
  "publishConfig": {
    "access": "public"
  },
  "@remirror": {
    "tsconfigs": {
      "src": {
        "compilerOptions": {
          "types": [
            "node"
          ]
        }
      }
    }
  }
}
